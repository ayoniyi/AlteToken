# Ethereum RPC endpoints 
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
# Alternative providers:
# SEPOLIA_RPC_URL=https://sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
# SEPOLIA_RPC_URL=https://rpc.ankr.com/eth_sepolia/YOUR_ANKR_KEY

# Private key of the account used to deploy (without 0x prefix)
PRIVATE_KEY=your_wallet_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Wallet addresses for contract constructor
TEAM_WALLET_ADDRESS=0x1111111111111111111111111111111111111111
RESERVE_WALLET_ADDRESS=0x2222222222222222222222222222222222222222 